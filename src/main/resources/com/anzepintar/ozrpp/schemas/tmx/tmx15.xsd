<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml.xsd"/>
  <xs:simpleType name="segtypes">
    <xs:restriction base="xs:token">
      <xs:enumeration value="block"/>
      <xs:enumeration value="paragraph"/>
      <xs:enumeration value="sentence"/>
      <xs:enumeration value="phrase"/>
    </xs:restriction>
  </xs:simpleType>
  <!-- ELEM                CONTENT -->
  <!--
    ATTR       
    NAME                TYPE           DEFAULT
  -->
  <!-- Base Document Element -->
  <xs:element name="tmx">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="header"/>
        <xs:element ref="body"/>
      </xs:sequence>
      <xs:attribute name="version" default="1.4">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="1.4"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <!-- Header -->
  <xs:element name="header">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="note"/>
        <xs:element ref="prop"/>
        <xs:element ref="ude"/>
      </xs:choice>
      <xs:attribute name="creationtool" use="required"/>
      <xs:attribute name="creationtoolversion" use="required"/>
      <xs:attribute name="segtype" use="required" type="segtypes"/>
      <xs:attribute name="o-tmf" use="required"/>
      <xs:attribute name="adminlang" use="required"/>
      <xs:attribute name="srclang" use="required"/>
      <xs:attribute name="datatype" use="required"/>
      <xs:attribute name="o-encoding"/>
      <xs:attribute name="creationdate"/>
      <xs:attribute name="creationid"/>
      <xs:attribute name="changedate"/>
      <xs:attribute name="changeid"/>
    </xs:complexType>
  </xs:element>
  <!-- Body -->
  <xs:element name="body">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="tu"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!-- No attributes -->
  <!-- Note -->
  <xs:element name="note">
    <xs:complexType mixed="true">
      <xs:attribute name="o-encoding"/>
      <xs:attribute ref="xml:lang"/>
      <xs:attribute name="lang"/>
    </xs:complexType>
  </xs:element>
  <!-- lang is deprecated: use xml:lang -->
  <!-- User-defined Encoding -->
  <xs:element name="ude">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="map"/>
      </xs:sequence>
      <xs:attribute name="name" use="required"/>
      <xs:attribute name="base"/>
    </xs:complexType>
  </xs:element>
  <!--
    Note: the base attribute is required if one or more <map>
    elements in the <ude> contain a code attribute.
  -->
  <!-- Character mapping -->
  <xs:element name="map">
    <xs:complexType>
      <xs:attribute name="unicode" use="required"/>
      <xs:attribute name="code"/>
      <xs:attribute name="ent"/>
      <xs:attribute name="subst"/>
    </xs:complexType>
  </xs:element>
  <!-- Property -->
  <xs:element name="prop">
    <xs:complexType mixed="true">
      <xs:attribute name="type" use="required"/>
      <xs:attribute ref="xml:lang"/>
      <xs:attribute name="o-encoding"/>
      <xs:attribute name="lang"/>
    </xs:complexType>
  </xs:element>
  <!-- lang is deprecated: use xml:lang -->
  <!-- Translation Unit -->
  <xs:element name="tu">
    <xs:complexType>
      <xs:sequence>
        <xs:choice minOccurs="0" maxOccurs="unbounded">
          <xs:element ref="note"/>
          <xs:element ref="prop"/>
        </xs:choice>
        <xs:element maxOccurs="unbounded" ref="tuv"/>
      </xs:sequence>
      <xs:attribute name="tuid"/>
      <xs:attribute name="o-encoding"/>
      <xs:attribute name="datatype"/>
      <xs:attribute name="usagecount"/>
      <xs:attribute name="lastusagedate"/>
      <xs:attribute name="creationtool"/>
      <xs:attribute name="creationtoolversion"/>
      <xs:attribute name="creationdate"/>
      <xs:attribute name="creationid"/>
      <xs:attribute name="changedate"/>
      <xs:attribute name="segtype" type="segtypes"/>
      <xs:attribute name="changeid"/>
      <xs:attribute name="o-tmf"/>
      <xs:attribute name="srclang"/>
    </xs:complexType>
  </xs:element>
  <!-- Translation Unit Variant -->
  <xs:element name="tuv">
    <xs:complexType>
      <xs:sequence>
        <xs:choice minOccurs="0" maxOccurs="unbounded">
          <xs:element ref="note"/>
          <xs:element ref="prop"/>
        </xs:choice>
        <xs:element ref="seg"/>
      </xs:sequence>
      <xs:attribute ref="xml:lang" use="required"/>
      <xs:attribute name="o-encoding"/>
      <xs:attribute name="datatype"/>
      <xs:attribute name="usagecount"/>
      <xs:attribute name="lastusagedate"/>
      <xs:attribute name="creationtool"/>
      <xs:attribute name="creationtoolversion"/>
      <xs:attribute name="creationdate"/>
      <xs:attribute name="creationid"/>
      <xs:attribute name="changedate"/>
      <xs:attribute name="o-tmf"/>
      <xs:attribute name="changeid"/>
      <xs:attribute name="lang"/>
    </xs:complexType>
  </xs:element>
  <!-- lang is deprecated: use xml:lang -->
  <!-- Text -->
  <xs:element name="seg">
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="bpt"/>
        <xs:element ref="ept"/>
        <xs:element ref="ph"/>
        <xs:element ref="it"/>
        <xs:element ref="hi"/>
        <xs:element ref="ut"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <!-- Content Markup ================================================== -->
  <xs:element name="bpt">
    <xs:complexType mixed="true">
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="sub"/>
      </xs:sequence>
      <xs:attribute name="i" use="required"/>
      <xs:attribute name="x"/>
      <xs:attribute name="type"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="ept">
    <xs:complexType mixed="true">
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="sub"/>
      </xs:sequence>
      <xs:attribute name="i" use="required"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="sub">
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="bpt"/>
        <xs:element ref="ept"/>
        <xs:element ref="it"/>
        <xs:element ref="ph"/>
        <xs:element ref="hi"/>
        <xs:element ref="ut"/>
      </xs:choice>
      <xs:attribute name="datatype"/>
      <xs:attribute name="type"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="it">
    <xs:complexType mixed="true">
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="sub"/>
      </xs:sequence>
      <xs:attribute name="pos" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:token">
            <xs:enumeration value="begin"/>
            <xs:enumeration value="end"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="x"/>
      <xs:attribute name="type"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="ph">
    <xs:complexType mixed="true">
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="sub"/>
      </xs:sequence>
      <xs:attribute name="x"/>
      <xs:attribute name="assoc"/>
      <xs:attribute name="type"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="hi">
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="bpt"/>
        <xs:element ref="ept"/>
        <xs:element ref="it"/>
        <xs:element ref="ph"/>
        <xs:element ref="hi"/>
        <xs:element ref="ut"/>
      </xs:choice>
      <xs:attribute name="x"/>
      <xs:attribute name="type"/>
    </xs:complexType>
  </xs:element>
  <!-- The <ut> element is deprecated -->
  <xs:element name="ut">
    <xs:complexType mixed="true">
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="sub"/>
      </xs:sequence>
      <xs:attribute name="x"/>
    </xs:complexType>
  </xs:element>
</xs:schema>
<!-- End -->
